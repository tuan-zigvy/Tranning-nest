{
  "name": "mentor-server",
  "version": "1.0.0",
  "description": "mentor-server",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:swc": "nest start -b swc -w",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "npx typeorm-ts-node-esm migration:generate -d ./data-source.ts",
    "migration:create": "typeorm migration:create",
    "migration:run": "npx typeorm-ts-node-esm migration:run -d ./data-source.ts",
    "migration:revert": "npx typeorm-ts-node-esm migration:revert -d ./data-source.ts"
  },
  "dependencies": {
    "@apollo/server": "^4.8.1",
    "@discord-nestjs/core": "^5.3.8",
    "@nestjs/apollo": "^12.0.7",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/graphql": "^12.0.8",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "argon2": "^0.30.3",
    "cache-manager": "^5.2.3",
    "cache-manager-redis-store": "^3.0.1",
    "cache-manager-redis-yet": "^4.1.2",
    "class-validator": "^0.14.0",
    "compression": "^1.7.4",
    "countries-and-timezones": "^3.5.1",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.9",
    "discord.js": "^14.11.0",
    "dotenv": "^16.3.1",
    "googleapis": "^122.0.0",
    "graphql": "^16.7.1",
    "graphql-scalars": "^1.22.2",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "log4js": "latest",
    "module-alias": "^2.2.3",
    "necord": "^6.0.0",
    "nestjs-redis": "git+https://github.com/GyanendroKh/nestjs-redis.git#nest8-fix",
    "nestjs-zod": "^2.3.3",
    "nodemailer": "^6.9.3",
    "passport": "^0.6.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.1",
    "redis": "^4.6.7",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.17",
    "webpack": "^5.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.1",
    "@nestjs/schematics": "^10.0.1",
    "@nestjs/testing": "^10.0.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.72",
    "@types/cors": "^2.8.13",
    "@types/debug": "^4.1.7",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.192",
    "@types/node": "^20.3.1",
    "@types/passport-facebook": "^3.0.0",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "_moduleAliases": {
    "@": "dist/src",
    "@user": "dist/src/user/",
    "@guard": "dist/src/auth/guard/",
    "@utils": "dist/src/utils/",
    "@redis": "dist/src/redis/",
    "@config": "dist/src/config/",
    "@auth": "dist/src/auth/"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
